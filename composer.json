{
    "name": "bernardomacedo/laravel-db-translator",
    "description": "Laravel 5 database translation extension",
    "keywords": [
        "bernardomacedo",
        "laravel-db-translator",
        "laravel",
        "translation"
    ],
    "homepage": "https://github.com/bernardomacedo/laravel-db-translator",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernardo Sousa de Macedo",
            "email": "bernardo.macedo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "5.3.x",
        "illuminate/translation": "5.3.x",
        "illuminate/database": "5.3.x",
        "illuminate/console": "5.3.x",
        "illuminate/filesystem": "5.3.x",
        "illuminate/config": "5.3.x",
        "illuminate/routing": "5.3.x"
    },
    "require-dev": {
        "phpunit/phpunit" : "^5",
        "orchestra/testbench": "3.2.x",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "bernardomacedo\\DBTranslator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bernardomacedo\\DBTranslator\\Test\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/bernardomacedo/laravel-db-translator/issues",
        "source": "https://github.com/bernardomacedo/laravel-db-translator"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
